{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "rules": {
    "strict": [2, "global"],
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-space-before-semi": 0,
    "no-extra-semi": 0,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": [2, {"properties": "always"}],
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 2,
    "semi": [2, "always"]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "require": true,
    "module": true,
    "__dirname": true,
    "process": true,
    "appRoot": true
  }
}